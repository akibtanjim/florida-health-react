    "axios": "^0.25.0",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "redux-persist": "^6.0.0",
